# Navigate to lib folder
cd docs/lib

# PDF.js files
wget https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.16.105/pdf.min.js -O pdf.min.js
wget https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.16.105/pdf.worker.min.js -O pdf.worker.min.js

# Tesseract.js files
wget https://cdnjs.cloudflare.com/ajax/libs/tesseract.js/4.1.1/tesseract.min.js -O tesseract.min.js
wget https://cdnjs.cloudflare.com/ajax/libs/tesseract.js/4.1.1/tesseract-core.wasm.js -O tesseract-core.wasm.js
wget https://cdnjs.cloudflare.com/ajax/libs/tesseract.js/4.1.1/tesseract-worker.min.js -O tesseract-worker.min.js

# Other required libraries
wget https://cdnjs.cloudflare.com/ajax/libs/docx/7.8.2/docx.min.js -O docx.min.js
wget https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.5/FileSaver.min.js -O FileSaver.min.js

# Language data files
cd ../tessdata
wget https://github.com/naptha/tessdata/raw/gh-pages/4.0.0/eng.traineddata -O eng.traineddata
wget https://github.com/naptha/tessdata/raw/gh-pages/4.0.0/fra.traineddata -O fra.traineddata
